@using AccurateAppend.Security
@using AccurateAppend.Websites.Clients.HtmlHelpers
<!DOCTYPE html>
<html>
<head>
    @Html.GetAnalyticsScript(Context.User.Identity.IsTestAccount())

    @Html.GetTitle()

    @Html.GetMetaDescription()

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link href="@Url.Content("~/Content/AccurateAppend_v7/css/bootstrap.css")" rel="stylesheet" type="text/css" media="screen" />
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

    @Html.CreateLoggingScript()

    <!-- Kendo Css -->
    <link href="//kendo.cdn.telerik.com/2020.2.513/styles/kendo.common.min.css" rel="stylesheet">
    <link href="//kendo.cdn.telerik.com/2020.2.513/styles/kendo.common-bootstrap.min.css" rel="stylesheet">
    @*<link href="//kendo.cdn.telerik.com/2020.2.513/styles/kendo.dataviz.min.css" rel="stylesheet">
        <link href="//kendo.cdn.telerik.com/2020.2.513/styles/kendo.dataviz.default.min.css" rel="stylesheet">*@
    <link href="//kendo.cdn.telerik.com/2020.2.513/styles/kendo.bootstrap.min.css" rel="stylesheet">

    <link href="@Url.Content("~/Content/AccurateAppend_v7/css/main.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/AccurateAppend_v7/css/kendo-ui.css")" rel="stylesheet" type="text/css" media="all" />

    @*<script src="//code.jquery.com/jquery-1.12.4.js"></script>*@
    <script src="//kendo.cdn.telerik.com/2020.2.513/js/jquery.min.js"></script>
    <script src="//kendo.cdn.telerik.com/2020.2.513/js/kendo.all.min.js"></script>
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    <!-- Kendo js-->
    @*<script src="//da7xgjtj801h2.cloudfront.net/2020.2.513/js/kendo.all.min.js"></script>*@

    <script src="//cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" crossorigin="anonymous" defer></script>
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" crossorigin="anonymous" defer></script>
    <script src="@Url.Content("~/Content/AccurateAppend_v7/js/jquery.inputmask.bundle.js")" defer></script>
    <script src="@Url.Content("~/Content/AccurateAppend_v7/js/jquery.main.js")" defer></script>

    <!-- Knockout js-->
    <script src="/scripts/knockout-3.4.2.js" type="text/javascript"></script>

    @Html.ZenDesk().GetWidgetScript(new HttpContextWrapper(HttpContext.Current))

    @RenderSection("HeadContent", false)
</head>
<body>
    <div id="wrapper">
        <!-- start header block -->
        @Html.Partial("AccurateAppend_v7/_Header")

        <!-- end header block -->
        <!-- start body container -->
        <main style="min-height: 500px;">
            @RenderBody()
        </main>
        <!-- end body container -->
        <!-- start footer block -->
        @Html.Partial("AccurateAppend_v7/_Footer")
        <!-- end footer block -->
    </div>
</body>
</html>