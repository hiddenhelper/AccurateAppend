@using AccurateAppend.Security
@using AccurateAppend.Websites.Clients.HtmlHelpers
<!DOCTYPE html>
<html>
<head>
    @Html.GetAnalyticsScript(Context.User.Identity.IsTestAccount())

    @Html.GetTitle()

    @Html.GetMetaDescription()

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/AccurateAppend_v6.1/css/bootstrap.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/AccurateAppend_v6.1/css/styles.css" rel="stylesheet" type="text/css" media="all" />
    
    @Html.CreateLoggingScript()

    <!-- Kendo js-->
    <script src="//da7xgjtj801h2.cloudfront.net/2015.2.902/js/jquery.min.js"></script>
    <script src="//da7xgjtj801h2.cloudfront.net/2015.2.902/js/kendo.all.min.js"></script>

    <!-- Kendo Css -->
    <link href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.common.min.css" rel="stylesheet">
    <link href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.common-bootstrap.min.css" rel="stylesheet">
    <link href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.dataviz.min.css" rel="stylesheet">
    <link href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.dataviz.default.min.css" rel="stylesheet">
    <link href="//kendo.cdn.telerik.com/2016.2.504/styles/kendo.bootstrap.min.css" rel="stylesheet">

    <!-- Knockout js-->
    <script src="/scripts/knockout-3.4.2.js" type="text/javascript"></script>

    @RenderSection("HeadContent", false)
</head>
<body>
    <!-- start header block -->
    @Html.Partial("AccurateAppend_v6.1/_Header")

    <!-- end header block -->
    <!-- start body container -->
    @RenderBody()
    <!-- end body container -->
    <!-- start footer block -->
    @Html.Partial("AccurateAppend_v6.1/_Footer")
    <!-- end footer block -->

    <!-- Start Visistat Logging Code -->
    @Html.VisiStat().GetVisiStatPixelSniffer(new HttpContextWrapper(HttpContext.Current))
    <!-- End Visistat Logging Code -->

    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
</body>
</html>